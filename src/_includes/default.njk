{% endblock %}
<!-- footer -->
{% block footer %}
<footer class="container ui-footer py-5" style="height:200px">
  {% include "./views/footer.njk" %}
</footer>
{% endblock %}
<!-- scripts  -->
{% include "./partials/footer-scripts.html" %}
