{% extends './base.njk' %} {% block head %}
<!-- Required meta tags -->
{% include "./partials/head.html" %}
<!--  -->
{% block nav %}
<div class="container-fluid bg-light">
  <nav>{% include "./views/nav.njk" %}</nav>
</div>
{% endblock %}
<!-- header -->
{% block header %}
<header class="container">{% include "./views/header.njk" %}</header>
